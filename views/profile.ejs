<!doctype html>
<html>
<head>
	<title>Node Authentication</title>
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
	<style>
		body 		{ padding-top:80px; word-wrap:break-word; }
	</style>
</head>
<body>
<div class="container">
	<div class="row">

        <div class="col-sm-4 col-sm-offset-4">
            <div class="well">
                <h3 class="text-primary"><span class="fa fa-instagram"></span> Instagram</h3>

                <p>
                    <strong>id</strong>: <%= user.u_id %><br>
                    <strong>token</strong>: <%= user.token %><br>
                    <strong>username</strong>: <%= user.username %><br>
                    <strong>followers</strong>: <%= user.followersCount %><br>
                    <strong>following</strong>: <%= user.followingCount %><br>
                    <img src="<%= user.profilePictureUrl %>" />
                </p>
                <a href="/logout">Logout</a>
            </div>
        </div>

	</div>

</div>
</body>
</html>